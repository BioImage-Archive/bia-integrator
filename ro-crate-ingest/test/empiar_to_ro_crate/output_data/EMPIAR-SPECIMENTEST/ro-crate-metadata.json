        {
            "@id": "_:Glucose control condition",
            "@type": [
                "bia:Protocol"
            ],
            "title": "Glucose control condition",
            "protocolDescription": "S. cerevisiae (BY4741) cells were grown in synthetic media supplemented with 20 mM dextrose (glucose control medium)."
        },
        {
            "@id": "_:Shared specimen",
            "@type": [
                "bia:Specimen"
            ],
            "biologicalEntity": [
                {
                    "@id": "_:S. cerevisiae"
                }
