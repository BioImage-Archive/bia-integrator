<html lang="en" class="vf-no-js">
    <head>
        <meta charset="UTF-8">
        <link rel="stylesheet" href="https://assets.emblstatic.net/vf/v2.5.8/css/styles.css">
        <meta charset="UTF-8">
        <title>Collection landing</title>
    </head>

<body class="vf-body | vf-stack vf-stack--400 embl-content-hub-loaded" data-vf-google-analytics-loaded="true" style="padding-bottom: 97px;"><div data-vf-js-banner-cookie-name="emblContentHub27360" data-vf-js-banner-cookie-version="224467" class="vf-banner vf-banner--fixed vf-banner--bottom vf-banner--notice" data-vf-js-banner="true" data-vf-js-banner-state="dismissible" data-vf-js-banner-button-text="I agree, dismiss this banner" data-vf-js-banner-id="2716530"><div class="vf-banner__content | vf-grid" data-vf-js-banner-text="">
        <p class="vf-text vf-text-body--2"> This website uses cookies, and the limited&nbsp;processing of your personal data to function. By using the site you are agreeing to this as outlined in our&nbsp;<a class="vf-banner__link" href="https://www.embl.org/info/privacy-policy/">Privacy Policy</a>. </p>
      <button class="vf-button vf-button--primary" data-vf-js-banner-close="">I agree, dismiss this banner</button></div></div>

  <header class="vf-header">
    
    <header class="vf-global-header">

                <div class="vf-global-header__inner">
          
                  <a href="https://www.ebi.ac.uk/bioimage-archive" class="vf-logo">
                    <img class="vf-logo__image" src="/bia-integrator-data/pages/assets/BIA-Logo.png" alt="BioImage Archive">
                    <span class="vf-logo__text">   BioImage Archive</span>
                  </a>
          
                </div>
          
            </header>
        </header>

<!-- Collection summary -->
<section class="vf-hero vf-hero--800 | vf-u-fullbleed" style="--vf-hero--bg-image: url('https://www.ebi.ac.uk/bioimage-archive/static/background_bioimage_archive.jpg');">
<div class="vf-hero__content | vf-card__content | vf-stack vf-stack--400">
        <h1 class="vf-hero__heading">{{ collection.title }}</h1>
        <p class="vf-hero__subheading">{{ collection.subtitle }}</p>
        <p class="vf-hero__text">{{ collection.description }}</p>
        </div>

</section>
 
<!-- Images -->
<section class="vf-card-container vf-card-container__col-3 | vf-u-background-color-ui--white vf-u-fullbleed" style="--vf-card__image--aspect-ratio: 16 / 9;">
  <div class="vf-card-container__inner">
 

        {% for study in studies %}

         <article class="vf-card vf-card--brand vf-card--bordered">
        <img src="{{ study.example_image_uri }}" alt="" class="vf-card__image" loading="lazy">
        <div class="vf-card__content | vf-stack vf-stack--400"><h3 class="vf-card__heading"><a class="vf-card__link" href="{{ study.accession_id }}.html">{{ study.accession_id }}       <svg aria-hidden="true" class="vf-card__heading__icon | vf-icon vf-icon-arrow--inline-end" width="1em" height="1em" xmlns="http://www.w3.org/2000/svg"><path d="M0 12c0 6.627 5.373 12 12 12s12-5.373 12-12S18.627 0 12 0C5.376.008.008 5.376 0 12zm13.707-5.209l4.5 4.5a1 1 0 010 1.414l-4.5 4.5a1 1 0 01-1.414-1.414l2.366-2.367a.25.25 0 00-.177-.424H6a1 1 0 010-2h8.482a.25.25 0 00.177-.427l-2.366-2.368a1 1 0 011.414-1.414z" fill="currentColor" fill-rule="nonzero"></path></svg>
        </a></h3>
        <p class="vf-card__text">{{ study.imaging_type }}
        <br><br>
        {{ study.organism }}
        <br><br>
        <b>Tags: </b>{{ study.tags|join(", ") }}
        <br><br>
        </p>
        </div>
        </article>
                  
        {% endfor %}

</section>        



          </div>
    </body>
</html>
